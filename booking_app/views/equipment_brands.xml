<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <data>
        <record id="brands_form" model="ir.ui.view">
            <field name="name">room.equipment.brands.form</field>
            <field name="model">room.equipment.brands</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="brands_tree" model="ir.ui.view">
            <field name="name">room.equipment.brands.tree</field>
            <field name="model">room.equipment.brands</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>


       <record id="list_brands" model="ir.actions.act_window">
            <field name="name">Nhãn hiệu</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">room.equipment.brands</field>
            <field name="view_mode">tree,form</field>
       </record>

    </data>
</odoo>
